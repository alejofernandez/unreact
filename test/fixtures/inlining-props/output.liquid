<div class="container">
  <p>This {{ name }} list is awesome
    <strong>{{ name }} is awesome</strong>
  </p>

  <ul>
    {% for item in list %}
      <li>{{ item }}</li>
    {% endfor %}
  </ul>

  <button class="btn btn-light">Submit</button>

  <div>
    {% if error and "E-mail must be valid." %}
      <p class="ulp-input-message ulp-input-errormsg">E-mail must be valid.</p>
    {% endif %}
  </div>
</div>
